  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["arunapi.main:app", "--reload"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "arunapi/app.py",
        "FLASK_ENV": "development",
        "REDIS_URL": "redis://127.0.0.1:6379"
      },
      "args": ["run", "--no-debugger"],
      "jinja": true,
      "justMyCode": true
    }
  ]
}
